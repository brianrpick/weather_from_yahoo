<%= form_tag '/custom', method: :get do%>
  <div>
    <%=label_tag :city, "City:"%>
    <%=text_field_tag :city %>
  </div>
  <div>
    <%=label_tag :state, "Two letter state abbreviation:"%>
    <%=text_field_tag :state%>
  </div>
  <div>
    <%= submit_tag "Submit"%>
  </div>
<%end%>
<%=@current_loc%>
<%@temps[0..4].each do |temp|%>
  <h1>Date: <%=temp["date"]%></h1>
  <h2>Day of the week:<%=temp["day"]%></h2>
  <h3>High Temp:<%=temp["high"]%></h3>
  <h3>Low Temp:<%=temp["low"]%></h3>
  <h3>Conditions:<%=temp["text"]%></h3>
<%end%>
